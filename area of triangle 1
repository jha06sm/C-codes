//c program to print area of triangle when sides of triangle is given
#include <stdio.h>
#include<math.h>
int main()
{
    double a,b,c;
    double s,area;
    printf("enter first sides of triangle:");
    scanf("%lf",&a);
    printf("enter second sides of triangle:");
    scanf("%lf",&b);
    printf("enter third sides of triangle:");
    scanf("%lf",&c);
    s=(a+b+c)/2;
    area=sqrt(s*(s-a)*(s-b)*(s-c));
    printf("area of trianle is:%lf\n",area);
    return 0;
}
