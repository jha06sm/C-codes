#include<stdio.h>
int main()
{
    int a[3][3],i,j,even=0,odd=0;
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            printf("enter the elements\n");
            scanf("%d",&a[i][j]);
        }
        printf("array elements are:");
        for(i=0;i<3;i++)
        {
            printf("\n");
            for(j=0;j<3;j++)
            printf("%d\t",a[i][j]);
        }
    }
        for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            if(a[i][j]%2==0)
            even++;
            else
            odd++;
        } 
    }
    printf("\n total even number are:%d\n total odd numbers are:%d",even,odd);
  return 0;
}
