#include<stdio.h>
int main()
{
    int a[10],sum=0,pro=1,i;
    for(i=0;i<10;i++)
    {
    printf("enter the number");
    scanf("%d",&a[i]);
    }
    for(i=0;i<10;i++)
    {
        if(a[i]%2==0)
        {
            sum+=a[i];
        }
        else
        {
            pro=pro*a[i];
        }
        
    }
    printf("the sum of even numbers is %d and product of odd numbers is %d",sum,pro);
    return 0;
}
