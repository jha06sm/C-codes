#include <stdio.h>
int main()
{
    double n1,n2,n3;
    printf("enter three number:");
    scanf("%lf %lf %lf",&n1,&n2,&n3);
    if(n1>=n2 && n1>=n3)
    {
    printf("%lf is largest",n1);
    }
    if(n2>=n1 && n2>=n3){
    printf("%lf is largest",n2);
    }
    else{
    printf("%lf is largest",n3);
    }
    return 0;

}
