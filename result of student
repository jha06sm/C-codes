#include <stdio.h>
int main()
{
    int i,j,x,arr[10];
    printf("enter marks of 10 students\n");
    for (i=0;i<10;i++)
    {
        scanf("%d",&arr[i]);
    }
    for(i=0;i<9;i++)
    {
        x=arr[i];
        for(j=i+1;j<10;j++)
        {
            if(x==arr[j])
            printf("%d number appeared more than once \n",x);
        }
    }
    return 0;
}
