#include <stdio.h>
int main()
{
    int num,i,temp=0;
    printf("enter any number to check for prime ");
    scanf("%d",&num);
    for(i=2;i<=num;i++)
    if(num%2==0)
    {
        temp++;
        break;
    }
    if(temp==0 && num!=1)
    {
        printf("%d is a prime number",num);
    }
    else
    {
        printf("number is not prime");
    }
    return 0;
}
