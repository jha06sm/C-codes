// c program to print limit of float
#include<limits.h>
#include<stdio.h>
int main()
{
printf("minimum float value =%f\n"
    "maximum float value=%f\n"
    "size of float in byte =%zu\n"
    "size of float in bits=%zu",
    __FLT_MIN__,__FLT_MAX__,__SIZEOF_FLOAT__,
    __SIZEOF_FLOAT__*CHAR_BIT);
    return 0;
}
